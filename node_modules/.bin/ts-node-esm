#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../packages/backend_que_funciona_con_el_application_manager/node_modules/ts-node/dist/bin-esm.js" "$@"
  ret=$?
else 
  node  "$basedir/../../packages/backend_que_funciona_con_el_application_manager/node_modules/ts-node/dist/bin-esm.js" "$@"
  ret=$?
fi
exit $ret
